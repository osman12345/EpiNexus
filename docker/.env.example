# EpiNexus Docker environment variables
# Copy this file to .env and set secure values before running docker compose

# Database credentials (REQUIRED - no defaults)
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Redis credentials (REQUIRED - no defaults)
REDIS_PASSWORD=your_secure_redis_password_here

# CORS origins (optional, defaults to localhost)
# CORS_ORIGINS=http://localhost:8501,https://your-domain.com
